{
    "manifest_version": 3,
    "name": "CIT Portal Revanced",
    "version": "1.0",
    "description": "Unipaの添付ファイルをダウンロードせずにプレビューします。",
    

    "host_permissions": [
      "https://portal.it-chiba.ac.jp/*",
      "https://portal.chibatech.ac.jp/*"
    ],
    
"content_scripts": [
  {
    "matches": [
    
      "*://portal.it-chiba.ac.jp/*",
      "*://portal.chibatech.ac.jp/*"
    ],
    "js": ["content-script.js"] 
  }
],
    

    "background": {
      "service_worker": "background.js" 
    }
  }